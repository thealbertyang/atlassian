{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Extension (HMR)",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": ["--extensionDevelopmentPath=${workspaceFolder}"],
      "outFiles": ["${workspaceFolder}/out/**/*.js"],
      "preLaunchTask": "dev:hmr",
      "env": {
        "ATLASSIAN_WEBVIEW_DEV_SERVER_URL": "http://localhost:5173"
      }
    }
  ]
}
